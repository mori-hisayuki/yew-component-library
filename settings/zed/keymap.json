[
  {
    "context": "Editor && VimControl && !VimWaiting && !menu",
    "bindings": {
      "up": null,
      "down": null,
      "left": null,
      "right": null,
      "backspace": null,
      "ctrl-h": "pane::ActivatePrevItem",
      "ctrl-l": "pane::ActivateNextItem"
    }
  },
  {
    "context": "Editor && vim_mode == insert",
